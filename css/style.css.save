  color: #fff;
  margin-bottom: 2px;
  position: relative;
  display: block;
  padding: 10px 15px;
  font-family: 'sanchezregular',serif;
  font-weight: 700;
}

li.expanded.dropdown > ul {
  padding: 0;
  margin-bottom: 20px;
}

li.expanded.dropdown > ul > li {
  position: relative;
  display: block;
  padding: 10px 15px;
  margin-bottom: -1px;
  border: 1px solid #DDD;
}

.navbar-right {
